n    </div>\n</template>\n\n\n<script>\nexport default {\n    name:'custom-input',\n    props:{\n        label:{\n            type:String,\n            required:true,\n        },\n        type:{\n            type:String,\n            default:'text'\n        },\n        value:{\n            type:String,\n            required:true,\n        }\n    },\n    data(){\n        return {\n            placeholder:'',\n            isLabelVisible:false,\n        }\n    },\n    created(){\n        this.placeholder=this.label;\n    },\n    methods:{\n        handleFocus(){\n            this.isLabelVisible=true;\n            this.placeholder='';\n        },\n        handleBlur(){\n            this.isLabelVisible=false;\n            this.placeholder=this.label;\n        },\n        handleChange(event){\n            this.$emit('input',event.target.value);\n        }\n    }\n}\n</script>\n\n\n<style lang=\"scss\">\n.custom-input{\n    display: flex;\n    flex-direction: column;\n\n    label{\n        font-size: 14px;\n        text-align: left;\n        padding-left:5px;\n        color: #79a8a9; \n    }\n\n    input{\n        border:none;\n        padding: 5px;\n        background: none;\n        border-bottom: 1px #d9dedeb8 solid;\n        outline: none;\n        margin-bottom: 15px;\n    }\n}\n</style>\n"]}]}